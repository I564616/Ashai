<%@ taglib prefix="c" uri="jakarta.tags.core"%>
<%@ taglib prefix="spring" uri="http://www.springframework.org/tags"%>
<%@ taglib prefix="theme" tagdir="/WEB-INF/tags/shared/theme"%>

<ul class="select-quantity clearfix">
	<li class="down disabled"><i class="icon-minus" grunticon-data-embed></i></li>
	<li><input class="qty-input" type="tel" value="1" data-minqty="1"maxlength="3" pattern="\d*"></li>
	<li class="up"><i class="icon-plus" grunticon-data-embed></i></li>
</ul>
