<?xml version="1.0" encoding="utf-8"?>
<process xmlns="http://www.hybris.de/xsd/processdefinition" start="approvalProcessCompleteAction" name="accApproval" processClass="de.hybris.platform.b2b.process.approval.model.B2BApprovalProcessModel">
	
	
	<action id="approvalProcessCompleteAction" bean="approvalProcessCompleteAction">
		<transition name="OK" to="success" />
		<transition name="NOK" to="error" />
	</action>
	
	<end id="error" state="ERROR">All went wrong.</end>
	<end id="failed" state="FAILED">Order not placed.</end>
	<end id="success" state="SUCCEEDED">Order placed.</end>

</process>