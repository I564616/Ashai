<?xml version="1.0" encoding="UTF-8"?>

<wizard-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="wizard-config.xsd"
               showPrefilledValues="true" selectMode="true" createMode="true" displaySubtypes="true">
    <displayed-properties>
        <group qualifier="General" visible="true" initially-opened="true">
            <label key="cockpit.config.label.General"/>
            <property qualifier="CMSItem.name"/>
        </group>
        <group qualifier="Properties" visible="true" initially-opened="true">
            <label key="config.wizardConfig.properties"/>
            <property qualifier="SABMInformationBoxComponent.desktopTitle"/>
            <property qualifier="SABMInformationBoxComponent.textList"/>
            <property qualifier="SABMInformationBoxComponent.ctaLinks"/>
            <property qualifier="SABMInformationBoxComponent.boundary" />
            <property qualifier="SABMInformationBoxComponent.boundaryType" />
            <property qualifier="SABMInformationBoxComponent.fit" />
            <property qualifier="SABMInformationBoxComponent.ctaAlignment" />

            <property qualifier="SABMInformationBoxComponent.titleIconDesktop"
                      editorCode="simpleMediaReferenceSelector">
                <parameter name="predefinedValuesStrategy" value="imageMediaPredefinedValuesStrategy"/>
            </property>
        </group>
    </displayed-properties>
</wizard-config>
