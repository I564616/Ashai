## SALESFORCE_LiveChatComponent for ALB - Staged

$contentCatalog=sgaContentCatalog
$contentCatalogName=SGA Content Catalog
$contentCV=catalogVersion(CatalogVersion.catalog(Catalog.id[default=$contentCatalog]),CatalogVersion.version[default=Staged])[default=$contentCatalog:Staged]

INSERT_UPDATE LiveChatComponent; $contentCV[unique = true]; uid[unique = true]; name; fromHour; toHour; fromMinute; toMinute; onlyOneRestrictionMustApply[default = 'true']
; ; livechatcomponent ; Live chat component ; 9 ; 16 ; 0 ; 59 ;


