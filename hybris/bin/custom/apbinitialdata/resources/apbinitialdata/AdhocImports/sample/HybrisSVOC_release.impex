## SALESFORCE_UserToVenueDataCronjob

$defaultLanguage = en

INSERT_UPDATE SalesForceExportCronjob; code[unique = true]; job(code); usertovenuedata ; sessionLanguage(isocode);active
; SalesForceReportExportJob_UserToVenueData ; SalesForceReportExportJob ;true; $defaultLanguage;false

INSERT_UPDATE Trigger; cronjob(code)[unique = true]; cronExpression;active
; SalesForceReportExportJob_UserToVenueData ;0 0 1 * * ?;false


