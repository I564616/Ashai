<xsd:schema targetNamespace="http://sap.com/xi/APPL"
			xmlns:p30="urn:gl.sabmiller.com:ecc:masterdata" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
			xmlns:p13="http://sap.com/xi/APPL/Global2" xmlns="http://sap.com/xi/APPL">
			<xsd:import namespace="urn:gl.sabmiller.com:ecc:masterdata" schemaLocation="main.xsd"/>
			<xsd:import namespace="http://sap.com/xi/APPL/Global2" schemaLocation="global2.xsd"/>
			<xsd:import namespace="http://sap.com/xi/APPL/Global2" />
			<xsd:import namespace="urn:gl.sabmiller.com:ecc:masterdata" />
			<xsd:simpleType name="PartyInternalID.Content">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">Proprietary
						identifier for a party</xsd:documentation>
				</xsd:annotation>
				<xsd:restriction base="xsd:token">
					<xsd:minLength value="1" />
					<xsd:maxLength value="32" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="LANGUAGEINDEPENDENT_MEDIUM_Name">
				<xsd:restriction base="xsd:string">
					<xsd:minLength value="0" />
					<xsd:maxLength value="40" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="BusinessScopeTypeCode.Content">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">Coded representation
						of the type of a BusinessScope</xsd:documentation>
				</xsd:annotation>
				<xsd:restriction base="xsd:token">
					<xsd:minLength value="1" />
					<xsd:maxLength value="4" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="PhoneNumberExtensionID">
				<xsd:restriction base="xsd:token">
					<xsd:minLength value="1" />
					<xsd:maxLength value="10" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="PhoneNumberAreaID">
				<xsd:restriction base="xsd:token">
					<xsd:minLength value="1" />
					<xsd:maxLength value="10" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="PartyStandardID.Content">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">Standardized
						identifier for a party, and the identification scheme used is
						controlled by an agency from the code list DE 3055
					</xsd:documentation>
				</xsd:annotation>
				<xsd:restriction base="xsd:token">
					<xsd:minLength value="1" />
					<xsd:maxLength value="13" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="PhoneNumberSubscriberID">
				<xsd:restriction base="xsd:token">
					<xsd:minLength value="1" />
					<xsd:maxLength value="30" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="Indicator">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">Indicator is the
						representation of a situation that has exactly two mutually
						exclusive Boolean values.</xsd:documentation>
				</xsd:annotation>
				<xsd:restriction base="xsd:boolean" />
			</xsd:simpleType>
			<xsd:simpleType name="CountryCode">
				<xsd:restriction base="xsd:token">
					<xsd:minLength value="2" />
					<xsd:maxLength value="3" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="BusinessScopeID.Content">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">Identifier of a
						BusinessScope</xsd:documentation>
				</xsd:annotation>
				<xsd:restriction base="xsd:token">
					<xsd:minLength value="1" />
					<xsd:maxLength value="36" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="MEDIUM_Name.Content">
				<xsd:restriction base="xsd:string">
					<xsd:minLength value="1" />
					<xsd:maxLength value="40" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="BusinessScopeInstanceID.Content">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">Identifier for the
						instance of a BusinessScope</xsd:documentation>
				</xsd:annotation>
				<xsd:restriction base="xsd:token">
					<xsd:minLength value="1" />
					<xsd:maxLength value="36" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="AgencyIdentificationCode">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">An
						AgencyIdentificationCode is a code representation of the agency
						responsible for a code list or identifier scheme, respectively
					</xsd:documentation>
				</xsd:annotation>
				<xsd:restriction base="xsd:token">
					<xsd:minLength value="1" />
					<xsd:maxLength value="3" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="LANGUAGEINDEPENDENT_LONG_Name">
				<xsd:restriction base="xsd:string">
					<xsd:minLength value="0" />
					<xsd:maxLength value="80" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="CountryDiallingCode">
				<xsd:restriction base="xsd:token">
					<xsd:minLength value="1" />
					<xsd:maxLength value="10" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="BusinessSystemID">
				<xsd:restriction base="xsd:token">
					<xsd:minLength value="1" />
					<xsd:maxLength value="60" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="ContactPersonInternalID.Content">
				<xsd:restriction base="xsd:token">
					<xsd:minLength value="1" />
					<xsd:maxLength value="32" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="LanguageCode">
				<xsd:restriction base="xsd:language">
					<xsd:minLength value="2" />
					<xsd:maxLength value="9" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="BusinessDocumentMessageIDContent">
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="35" />
					<xsd:minLength value="1" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType name="GLOBAL_DateTime">
				<xsd:restriction base="xsd:dateTime">
					<xsd:pattern
						value="[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(.[0-9]*)?(Z)" />
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:complexType name="MatlERPRplctnReqMsgMatlSlsPlntSpec">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">Material Master
						Replication Message: plant specific data in node &quot;Sales&quot;
					</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="PlantID" type="p13:PlantID" />
					<xsd:element name="LoadingMaterialGroupCode" type="p13:LoadingMaterialGroupCode"
						minOccurs="0" />
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="ContactPersonInternalID">
				<xsd:simpleContent>
					<xsd:extension base="ContactPersonInternalID.Content">
						<xsd:attribute name="schemeID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:minLength value="1" />
									<xsd:maxLength value="60" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="schemeAgencyID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:minLength value="1" />
									<xsd:maxLength value="60" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:extension>
				</xsd:simpleContent>
			</xsd:complexType>
			<xsd:complexType name="BusinessDocumentMessageHeaderParty">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">General information
						about a party that is responsible for sending or receiving a
						BusinessDocument at business application level</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="InternalID" type="PartyInternalID"
						minOccurs="0" />
					<xsd:element name="StandardID" type="PartyStandardID"
						minOccurs="0" maxOccurs="unbounded" />
					<xsd:element name="ContactPerson"
						type="BusinessDocumentMessageHeaderPartyContactPerson" minOccurs="0" />
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="MatlERPRplctnReqMsgMatl">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">Material
					</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="InternalID" type="p13:ProductInternalID" />
					<xsd:element name="ProductStandardID" type="p13:ProductStandardID"
						minOccurs="0" />
					<xsd:element name="PreviousProductInternalID" type="p13:ProductInternalID"
						minOccurs="0" />
					<xsd:element name="MaterialTypeCode" type="p13:MaterialTypeCode" />
					<xsd:element name="MaterialMerchandiseTypeCode" type="p13:Code"
						minOccurs="0" />
					<xsd:element name="MaterialIndustrialSectorCode" type="p13:MaterialIndustrialSectorCode" />
					<xsd:element name="DivisionCode" type="p13:DivisionCode"
						minOccurs="0" />
					<xsd:element name="SerialNumberProfileCode" type="p13:Code"
						minOccurs="0" />
					<xsd:element name="CatchWeightQuantityEntryProfileCode"
						type="p13:CatchWeightQuantityEntryProfileCode" minOccurs="0" />
					<xsd:element name="CatchWeightQuantityVarianceProfileCode"
						type="p13:CatchWeightQuantityVarianceProfileCode" minOccurs="0" />
					<xsd:element name="IndustryStandardName" type="xsd:string"
						minOccurs="0" />
					<xsd:element name="DeletedIndicator" type="p13:Indicator"
						minOccurs="0" />
					<xsd:element name="BatchManagementRequiredIndicator"
						type="p13:Indicator" />
					<xsd:element name="ValidityStartDate" type="p13:Date"
						minOccurs="0" />
					<xsd:element name="SystemAdministrativeData" type="p13:SystemAdministrativeData"
						minOccurs="0" />
					<xsd:element name="Description" type="MatlERPRplctnReqMsgMatlDesc"
						maxOccurs="unbounded" />
					<xsd:element name="TextCollection" type="p13:TextCollection"
						minOccurs="0" />
					<xsd:element name="ProductCategory" type="MatlERPRplctnReqMsgMatlProdCat"
						minOccurs="0" maxOccurs="unbounded" />
					<xsd:element name="GlobalTradeItemNumber"
						type="MatlERPRplctnReqMsgMatlGlobTrdItemNo" minOccurs="0"
						maxOccurs="unbounded" />
					<xsd:element name="QuantityUnit" type="MatlERPRplctnReqMsgMatlQtyUnit"
						maxOccurs="unbounded" />
					<xsd:element name="PlantSpecification" type="MatlERPRplctnReqMsgMatlPlntSpec"
						minOccurs="0" maxOccurs="unbounded" />
					<xsd:element name="Sales" type="MatlERPRplctnReqMsgMatlSls"
						minOccurs="0" />
					<xsd:element name="ForeignTradeInformation" type="MatlERPRplctnReqMsgMatlFrgnTradeInf"
						minOccurs="0" maxOccurs="unbounded" />
					<xsd:element name="HandlingMeans" type="MatlERPRplctnReqMsgMatlHndlgMeans"
						minOccurs="0" />
					<xsd:element name="Storage" type="MatlERPRplctnReqMsgMatlStor"
						minOccurs="0" />
					<xsd:element name="Packaging" type="MatlERPRplctnReqMsgMatlPackgg"
						minOccurs="0" />
					<xsd:group ref="p30:MatlERPRplctnReqMsgMatl" />
					<xsd:group ref="p30:MatlERPRplctnReqMsgMatl_V1" />
				</xsd:sequence>
				<xsd:attributeGroup ref="p30:MatlERPRplctnReqMsgMatl" />
				<xsd:attributeGroup ref="p30:MatlERPRplctnReqMsgMatl_V1" />
			</xsd:complexType>
			<xsd:complexType name="MatlERPRplctnReqMsgMatlFrgnTradeInf">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">Material Master
						Replication Message: node &quot;ForeignTradeInformation&quot;
					</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="PlantID" type="p13:PlantID" />
					<xsd:element name="OriginCountryCode" type="p13:CountryCode"
						minOccurs="0" />
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="MatlERPRplctnReqMsgMatlHndlgMeans">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">Material Master
						Replication Message: node &quot;Handling Means&quot;
					</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="WarehouseStorageBehaviourMethodID"
						type="p13:StorageBehaviourMethodID" minOccurs="0" />
					<xsd:element name="InspectionSpecificationID" type="p13:InspectionSpecificationID"
						minOccurs="0" />
					<xsd:element name="WarehouseMaterialHandlingCode" type="p13:MaterialHandlingCode"
						minOccurs="0" />
					<xsd:element name="HazardousSubstanceHazardCategoryCode"
						type="p13:HazardousSubstanceHazardCategoryCode" minOccurs="0" />
					<xsd:element name="WarehouseMaterialGroupCode" type="xsd:string"
						minOccurs="0" />
					<xsd:element name="EnvironmentallyRelevanceIndicator"
						type="p13:Indicator" minOccurs="0" />
					<xsd:element name="TheftRiskIndicator" type="p13:Indicator"
						minOccurs="0" />
					<xsd:element name="QuarantineMinimumDuration" type="p13:Duration"
						minOccurs="0" />
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="BusinessScopeID">
				<xsd:simpleContent>
					<xsd:extension base="BusinessScopeID.Content">
						<xsd:attribute name="schemeID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:minLength value="1" />
									<xsd:maxLength value="60" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="schemeAgencyID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:minLength value="1" />
									<xsd:maxLength value="60" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:extension>
				</xsd:simpleContent>
			</xsd:complexType>
			<xsd:complexType name="PartyInternalID">
				<xsd:simpleContent>
					<xsd:extension base="PartyInternalID.Content">
						<xsd:attribute name="schemeID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:minLength value="1" />
									<xsd:maxLength value="60" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="schemeAgencyID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:minLength value="1" />
									<xsd:maxLength value="60" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:extension>
				</xsd:simpleContent>
			</xsd:complexType>
			<xsd:complexType name="MatlERPRplctnReqMsgMatlPackgg">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">Material Master
						Replication Message: node &quot;Packaging&quot;
					</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="PackagingReferenceProductInternalID"
						type="p13:ProductInternalID" minOccurs="0" />
					<xsd:element name="PackagingMaterialTypeMaterialGroupCode"
						type="p13:PackagingMaterialTypeMaterialGroupCode" minOccurs="0" />
					<xsd:element name="PackagingMaterialTypeCode" type="p13:PackagingMaterialTypeCode"
						minOccurs="0" />
					<xsd:element name="DefaultHandlingUnitTypeCode" type="p13:HandlingUnitTypeCode"
						minOccurs="0" />
					<xsd:element name="HandlingUnitTypeCode" type="p13:HandlingUnitTypeCode"
						minOccurs="0" />
					<xsd:element name="DangerousGoodsIndicator" type="p13:Indicator"
						minOccurs="0" />
					<xsd:element name="TareWeightFixedIndicator" type="p13:Indicator"
						minOccurs="0" />
					<xsd:element name="ClosedPackagingIndicator" type="p13:Indicator"
						minOccurs="0" />
					<xsd:element name="AllowedPackagingLengthMeasure" type="p13:Measure"
						minOccurs="0" />
					<xsd:element name="AllowedPackagingWidhtMeasure" type="p13:Measure"
						minOccurs="0" />
					<xsd:element name="AllowedPackagingHeightMeasure" type="p13:Measure"
						minOccurs="0" />
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="BusinessScopeTypeCode">
				<xsd:simpleContent>
					<xsd:extension base="BusinessScopeTypeCode.Content">
						<xsd:attribute name="listID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:minLength value="1" />
									<xsd:maxLength value="60" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="listVersionID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:minLength value="1" />
									<xsd:maxLength value="15" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="listAgencyID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:minLength value="1" />
									<xsd:maxLength value="60" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="listAgencySchemeID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:minLength value="1" />
									<xsd:maxLength value="60" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="listAgencySchemeAgencyID" type="AgencyIdentificationCode" />
					</xsd:extension>
				</xsd:simpleContent>
			</xsd:complexType>
			<xsd:complexType name="EmailURI">
				<xsd:simpleContent>
					<xsd:extension base="xsd:anyURI">
						<xsd:attribute name="schemeID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:minLength value="1" />
									<xsd:maxLength value="60" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:extension>
				</xsd:simpleContent>
			</xsd:complexType>
			<xsd:complexType name="MatlERPRplctnReqMsgMatlStorPlntSpec">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">Material Master
						Replication Message: plant specific data in node
						&quot;Storage&quot;</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="PlantID" type="p13:PlantID" />
					<xsd:element name="PhysicalInventoryCycleCountingProfileCode"
						type="p13:PhysicalInventoryCycleCountingProfileCode" minOccurs="0" />
					<xsd:element name="PhysicalInventoryCycleCountingProfileFixedIndicator"
						type="p13:Indicator" minOccurs="0" />
					<xsd:element name="StorageMaximumDuration" type="p13:Duration"
						minOccurs="0" />
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="MatlERPRplctnReqMsgMatlDesc">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">Material Master
						Replication Message: node &quot;Packaging&quot;
					</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="Description" type="p13:SHORT_Description" />
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="PhoneNumber">
				<xsd:sequence>
					<xsd:element name="AreaID" type="PhoneNumberAreaID"
						minOccurs="0" />
					<xsd:element name="SubscriberID" type="PhoneNumberSubscriberID"
						minOccurs="0" />
					<xsd:element name="ExtensionID" type="PhoneNumberExtensionID"
						minOccurs="0" />
					<xsd:element name="CountryCode" type="CountryCode"
						minOccurs="0" />
					<xsd:element name="CountryDiallingCode" type="CountryDiallingCode"
						minOccurs="0" />
					<xsd:element name="CountryName" type="MEDIUM_Name"
						minOccurs="0" />
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="PartyStandardID">
				<xsd:simpleContent>
					<xsd:extension base="PartyStandardID.Content">
						<xsd:attribute name="schemeAgencyID" use="required">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:minLength value="1" />
									<xsd:maxLength value="3" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:extension>
				</xsd:simpleContent>
			</xsd:complexType>
			<xsd:complexType name="BusinessDocumentMessageID">
				<xsd:simpleContent>
					<xsd:extension base="BusinessDocumentMessageIDContent">
						<xsd:attribute name="schemeID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:maxLength value="60" />
									<xsd:minLength value="1" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="schemeAgencyID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:maxLength value="60" />
									<xsd:minLength value="1" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="schemeAgencySchemeAgencyID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:token">
									<xsd:maxLength value="3" />
									<xsd:minLength value="1" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:extension>
				</xsd:simpleContent>
			</xsd:complexType>
			<xsd:complexType name="MatlERPRplctnReqMsgMatlQtyUnit">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">Material Master
						Replication Message: node &quot;Quantity Unit&quot;
					</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="Quantity" type="p13:Quantity" />
					<xsd:element name="MaterialBaseQuantity" type="p13:Quantity" />
					<xsd:element name="BaseQuantityUnitIndicator" type="p13:Indicator"
						minOccurs="0" />
					<xsd:element name="MaterialWeightsDimensionsVolume"
						type="MatlERPRplctnReqMsgMatlQtyUnitWeightsDimsVol" minOccurs="0"
						maxOccurs="unbounded" />
					<xsd:element name="StackingMaximumQuantity" type="p13:Quantity"
						minOccurs="0" />
					<xsd:element name="RemainingNestingVolumePercent" type="p13:Percent"
						minOccurs="0" />
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="MatlERPRplctnReqMsgMatlStor">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">Material Master
						Replication Message: node &quot;Storage&quot;</xsd:documentation>
				</xsd:annotation>
				<xsd:sequence>
					<xsd:element name="StorageBehaviourMethodID" type="p13:StorageBehaviourMethodID"
						minOccurs="0" />
					<xsd:element name="LogisticsContainerRequirementsCode"
						type="p13:LogisticsContainerRequirementsCode" minOccurs="0" />
					<xsd:element name="MaterialLabelingCode" type="p13:Code"
						minOccurs="0" />
					<xsd:element name="TemperatureRequirementsCode" type="p13:Code"
						minOccurs="0" />
					<xsd:element name="TotalShelfLifeMaximumDuration" type="p13:Duration"
						minOccurs="0" />
					<xsd:element name="RemainingShelfLifeMinimumDuration"
						type="p13:Duration" minOccurs="0" />
