def showOrdersByMonth(UserModel user, B2BUnitModel unit, FlexibleSearchService flexibleSearchService, Record record, Dates dates) {
    String query = ""select FORMAT({creationtime},'MMM,yyyy'), count(FORMAT({creationtime}, 'MMM,yyyy')) from {order} where {user} = ?user and {unit} = ?unit group by FORMAT({creationtime}, 'MMM,yyyy') order by {creationtime}""
    FlexibleSearchQuery fsQuery = new FlexibleSearchQuery(query)
    fsQuery.addQueryParameter(""user"", user)
    fsQuery.addQueryParameter(""unit"", unit)
    fsQuery.setResultClassList(Arrays.asList(String.class, Integer.class))
    SearchResult<List<?>> searchResult = flexibleSearchService.search(fsQuery)
    int totalOrders = 0
    searchResult.result.each {
        totalOrders += it.get(1)
        SimpleDateFormat formatter = new SimpleDateFormat(""MMM, yyyy"")
        Date date = formatter.parse(it.get(0))
        record.orderNoByMonth.put(formatter.format(date), it.get(1))
        dates.set.add(date)
    }
    record.totalOrders = totalOrders

    String samAccessQuery = ""select {pk} from {AsahiSAMAccess} where {payer}=?unit and {b2bCustomer}=?user""
    FlexibleSearchQuery accessQuery = new FlexibleSearchQuery(samAccessQuery)
    accessQuery.addQueryParameter(""unit"", unit)
    accessQuery.addQueryParameter(""user"", user)

    SearchResult<AsahiSAMAccessModel> result = flexibleSearchService.search(accessQuery)
    if (result.getResult().size() > 0) {
        AsahiSAMAccessModel accessModel = result.result.get(0)
        record.payAccess = accessModel.payAccess
        record.orderAccess = accessModel.orderAccess
    }
}

