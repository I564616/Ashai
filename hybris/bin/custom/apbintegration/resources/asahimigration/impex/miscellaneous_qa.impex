private static boolean hasSamAccess(List<AsahiSAMAccessModel> accesses, AsahiB2BUnitModel customer){
    boolean exist = false;
    for(AsahiSAMAccessModel it: accesses){
        if(null != it.getPayer() && it.getPayer().getUid().trim().equals(customer.getUid().trim())){
            exist = true;
            break;
        }
    }
    return exist;
}

